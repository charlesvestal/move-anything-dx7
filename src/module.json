{
  "id": "dexed",
  "name": "Dexed",
  "abbrev": "DX",
  "version": "0.5.3",
  "description": "6-operator FM synthesizer (Dexed/MSFA) with .syx patch support",
  "author": "Dexed/MSFA by google/asb2m10 (port: charlesvestal)",
  "license": "GPL-3.0",
  "ui": "ui.js",
  "dsp": "dsp.so",
